#! /usr/bin/env bash

# DESCRIPTION
# Installs applications.

# EXECUTION
# Alfred
install_dmg_app "$ALFRED_APP_URL" "$ALFRED_VOLUME_NAME" "$ALFRED_APP_NAME"

# CheatSheet
install_zip_app "$CHEATSHEET_APP_URL" "$CHEATSHEET_APP_NAME"

# DashCamViewer
install_dmg_app "$DASHCAMVIEWER_APP_URL" "$DASHCAMVIEWER_APP_NAME"

# iStat Menus
install_zip_app "$ISTATS_APP_URL" "$ISTATS_APP_NAME"

# iTerm
install_zip_app "$ITERM_APP_URL" "$ITERM_APP_NAME"

# Knox
install_zip_app "$KNOX_APP_URL" "$KNOX_APP_NAME"

# ksdiff
install_zip_pkg "$KSDIFF_APP_URL" "$KSDIFF_APP_NAME"

# Marp
install_dmg_app "$MARP_APP_URL" "$MARP_VOLUME_NAME" "$MARP_APP_NAME"

# Micro Snitch
install_zip_app "$MICRO_SNITCH_APP_URL" "$MICRO_SNITCH_APP_NAME"

# OmniGraffle
install_dmg_app "$OMNIGRAFFLE_APP_URL" "$OMNIGRAFFLE_VOLUME_NAME" "$OMNIGRAFFLE_APP_NAME"

# OpenOffice
install_dmg_app "$OPEN_OFFICE_APP_URL" "$OPEN_OFFICE_VOLUME_NAME" "$OPEN_OFFICE_APP_NAME"

# Opera
install_dmg_app "$OPERA_APP_URL" "$OPERA_VOLUME_NAME" "$OPERA_APP_NAME"

# Path Finder
install_zip_app "$PATH_FINDER_APP_URL" "$PATH_FINDER_APP_NAME"

# Paw
install_zip_app "$PAW_APP_URL" "$PAW_APP_NAME"

# PG Admin
install_dmg_app "$PG_ADMIN_APP_URL" "$PG_ADMIN_VOLUME_NAME" "$PG_ADMIN_APP_NAME"

# PSequel
install_zip_app "$PSEQUEL_APP_URL" "$PSEQUEL_APP_NAME"

# QuickLook - Markdown
install_zip_app "$QUICK_LOOK_MARKDOWN_APP_URL" "$QUICK_LOOK_MARKDOWN_APP_NAME"

# RescueTime
install_dmg_app "$RESCUE_TIME_APP_URL" "$RESCUE_TIME_VOLUME_NAME" "$RESCUE_TIME_APP_NAME"

# Sonos
install_dmg_app "$SONOS_APP_URL" "$SONOS_VOLUME_NAME" "$SONOS_APP_NAME"

# Sublime Text
install_dmg_app "$SUBLIME_TEXT_APP_URL" "$SUBLIME_TEXT_VOLUME_NAME" "$SUBLIME_TEXT_APP_NAME"

# Sublime Text URL Handler
install_zip_app "$SUBLIME_URL_HANDLER_APP_URL" "$SUBLIME_URL_HANDLER_APP_NAME"

# SurfEasy VPN
install_dmg_app "$SURF_EASY_VPN_APP_URL" "$SURF_EASY_VPN_VOLUME_NAME" "$SURF_EASY_VPN_APP_NAME"

# Tor Browser
install_dmg_app "$TOR_BROWSER_APP_URL" "$TOR_BROWSER_VOLUME_NAME" "$TOR_BROWSER_APP_NAME"

# Trailer
install_zip_app "$TRAILER_APP_URL" "$TRAILER_APP_NAME"

# Transmit
install_zip_app "$TRANSMIT_APP_URL" "$TRANSMIT_APP_NAME"

# Viscosity
install_dmg_app "$VISCOSITY_APP_URL" "$VISCOSITY_VOLUME_NAME" "$VISCOSITY_APP_NAME"

# Visual Studio Code
install_zip_app "$VISUAL_STUDIO_CODE_APP_URL" "$VISUAL_STUDIO_CODE_APP_NAME"

# Vivaldi
install_dmg_app "$VIVALDI_APP_URL" "$VIVALDI_VOLUME_NAME" "$VIVALDI_APP_NAME"

# VLC
install_dmg_app "$VLC_APP_URL" "$VLC_VOLUME_NAME" "$VLC_APP_NAME"
